<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Homespun Dev is a personal portfolio site for Mary Frey a student in the Full-Stack Development Track at Centriq. It features some of the skills I've learned in the course as well as some personal projects. ">
    <meta name="author" content="Mary Frey">
    <meta name="keywords" content="Homespun Dev, developer, full-stack, C#, mobile web design, responsive web design, sql, ef, entity framework, ef, .net" />

    <title>@ViewBag.Title | Homespun Dev</title>

    <link rel="icon" href="~/Content/Images/HDtypewriter75.png" type="image/x-icon" />
    <!-- Bootstrap core CSS -->
    <link href="~/Content/Styles/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Josefin+Slab:100,300,400,600,700,100italic,300italic,400italic,600italic,700italic" rel="stylesheet" type="text/css">

    <!-- Custom styles for this template -->
    <link href="~/Content/Styles/business-casual.css" rel="stylesheet">
    <!--Lity Lightbox CSS-->
    <link href="~/Content/Styles/lity.min.css" rel="stylesheet" />
    <link href="~/Content/Styles/custom.css" rel="stylesheet" />
    <script src="https://use.fontawesome.com/18436fa9ec.js"></script>
</head>

<body>

    <!--<div class="tagline-upper text-center text-heading text-shadow text-white mt-5 d-none d-lg-block">Business Casual</div>
    <div class="tagline-lower text-center text-expanded text-shadow text-uppercase text-white mb-5 d-none d-lg-block">3481 Melrose Place | Beverly Hills, CA 90210 | 123.456.7890</div>-->
    <!-- Navigation -->
    <div id="outernav">
        <nav id="outernav2" class="navbar navbar-left navbar-expand-lg navbar-expand-md navbar-expand-sm navbar-light bg-faded py-lg-4">

            <div class="container mainmenu" id="nav">
                <a class="navbar-brand" href="~/Home/Index"><img id="minilogo" src="~/Content/Images/HD1L353by79-min.png" /></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarResponsive">
                    <ul class="navbar-nav">
                        <!--<li class="nav-item px-lg-4">
                          <a class="nav-link text-uppercase text-expanded" href="index.html">Home

                          </a>
                        </li>-->
                        <li class="nav-item px-lg-4">
                            <a class="nav-link text-uppercase text-expanded" href="~/Home/Projects">Projects</a>
                        </li>
                        <li class="nav-item px-lg-4">
                            <a class="nav-link text-uppercase text-expanded" href="~/Home/Resume">Resume</a>
                        </li>
                        <li class="nav-item px-lg-4">
                            <a class="nav-link text-uppercase text-expanded" href="~/Home/Links">Links</a>
                        </li>

                        <li class="nav-item px-lg-4">
                            <a class="nav-link text-uppercase text-expanded" href="~/Home/Contact">Contact</a>
                        </li>

                    </ul>
                </div>
            </div>
            <hr class="divider" />
        </nav>

        @RenderSection("Nav2", false);
    </div>

    <div class="container body-content">
        @RenderBody()

        <hr />
        <footer class="bg-faded text-center">
            <div class="container">
                <p class="m-0">Copyright &copy; Mary A. Frey 2017</p>

            </div>
        </footer>


        <script src="~/Content/Scripts/jquery/jquery.min.js"></script>
        <script src="~/Content/Scripts/popper/popper.min.js"></script>
        <script src="~/Content/Scripts/bootstrap.min.js"></script>
        <script src="~/Content/Scripts/typed.js"></script>
        <script src="~/Content/Scripts/lity.min.js"></script>
        <script src="~/Content/Scripts/custom.js"></script>
        <!-- jQuery UI -->
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
        @* jQuery Validation *@
        @* This was dded to enable the client side form validation that
            is added by the Model Data Annotations *@
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
        <script>
            var typed2 = new Typed('.typed2', {
                strings: ['Welcome To^7000 ', '^50000'],
                typeSpeed: 60,
                cursorChar: '',
                loop: true
            });
        </script>
        <script>
            var typed3 = new Typed('.typed3', {
                strings: ['^2000Homespun Dev', 'Homespun Dev:<br/>A Personal Portfolio Site', 'I am Mary Frey', 'A Full-Stack Developer', 'Skilled in:<br/><i>Mobile / Responsive Web Design</i>',
                'Skilled in:<br/><i>HTML5</i>', 'Skilled in:<br/><i>CSS3</i>', 'Skilled in:<br/><i>JavaScript</i>', 'Skilled in:<br/><i>jQuery</i>',
                /*'Skilled in:<br/><i>jQuery UI</i>',*/ 'Skilled in:<br/><i>MVC Framework</i>', 'Skilled in:<br/><i>.NET Framework</i>', 'Skilled in:<br/><i>C#</i>', 'Skilled in:<br/><i>ASP.NET</i>',
                /*'Skilled in:<br/><i>Visual Studio</i>',*/ 'Skilled in:<br/><i>SQL</i>', /*'Skilled in:<br/><i>SQL Server</i>',*/ 'I&#39;m Looking for New Opportunities', 'Let&#39;s Talk!^10000'],
                typeSpeed: 65,
                backSpeed: 40,
                smartBackspace: true, // this is a default
                loop: true
            });
        </script>
        <script>
            $(document).ready(function () {
                $('.body-content').css('margin-top', $('#outernav').height() + 'px');
            });

            $(window).resize(function () {
                $('.body-content').css('margin-top', $('#outernav').height() + 'px');
            });
        </script>
        <script>
            //script to highlight the selected navigation
            $(function () {
                //the element that we are attempting to target
                $('.mainmenu ul li').each(function () {
                    if ($(this).find('a').attr('href') == window.location.pathname) {
                        //in the case that they attach the class to the anchor tag:
                        //$('.mainmenu ul li a').each...
                        //if($(this).attr('href')==...

                        //whatever class name is in css
                        //if there isn't one, you may have to create one
                        //you can attempt to use whatever they do for hovering...if it exists
                        $(this).addClass('selected');
                    }
                });
            });
        </script>
        @RenderSection("Scripts", false)
    </div>
</body>
</html>
